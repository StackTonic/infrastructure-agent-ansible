#jinja2:lstrip_blocks: True
## {{ ansible_managed }}
integration_name: com.newrelic.postgresql

instances:
{% for instance in nrinfragent_postgresql_instances %}
  - name: {{ instance.name }}
    command: {{ instance.command }}
    arguments:
    {% for argument in instance.arguments %}
      {{ argument }}
    {% endfor %}
    labels:
    {% for label in instance.labels %}
      {{ label }}
    {% endfor %}
{% endfor %}
